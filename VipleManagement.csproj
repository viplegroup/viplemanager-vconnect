    <RootNamespace>VipleManagement</RootNamespace>
    <AssemblyName>VipleManagement</AssemblyName>
    <ApplicationIcon>viple_ressources\images\viple.ico</ApplicationIcon>
    <Company>Viple SAS</Company>
